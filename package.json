{
  "name": "polymer-playerlytics",
  "version": "0.0.2",
  "description": "Playerlytics is a player polymer component wrapper",
  "keywords": [
    "npm-package",
    "package",
    "es6",
    "colelctions",
    "array",
    "arrays",
    "lodash",
    "underscore",
    "utility"
  ],
  "license": "MIT",
  "author": "Adam Gedney",
  "homepage": "https://github.com/OlympusatDevelopment/polymer-playerlytics",
  "repository": {
    "type": "git",
    "url": "https://github.com/OlympusatDevelopment/polymer-playerlytics"
  },
  "bugs": {
    "mail": "adam.gedney@gmail.com",
    "url": "https://github.com/OlympusatDevelopment/polymer-playerlytics/issues"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./.nyc_output ./.tmp ./coverage ./lib",
    "dev": "node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.build.development.js",
    "build": "node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.build.production.js",
    "test": "mocha-webpack --webpack-config webpack.config.test.js \"test/runner.js\"",
    "test:watch": "mocha-webpack --webpack-config webpack.config.test.js \"test/runner.js\" --watch",
    "cover": "nyc --reporter=lcov --reporter=text npm run test",
    "coveralls": "nyc --reporter=lcov --reporter=text npm run test && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint src test",
    "prepublish": "npm run clean && npm run test && npm run build",
    "start" :"npm run test:watch"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "instrument": false,
    "sourceMap": false
  },
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.1",
    "coveralls": "^2.13.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "fs-file-tree": "^1.0.4",
    "istanbul-instrumenter-loader": "^2.0.0",
    "mocha": "^3.4.2",
    "mocha-webpack": "^0.7.0",
    "nyc": "^11.0.1",
    "rimraf": "^2.6.1",
    "sinon": "^2.3.2",
    "webpack": "^2.6.1",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0",
    "webpack-uglify-js-plugin": "^1.1.9"
  }
}
